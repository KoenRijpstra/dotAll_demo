{
  "require": {
    "craftcms/cms": "^3.0.0",
    "vlucas/phpdotenv": "^2.4.0",
    "benjamminf/craft-neo": "dev-dev-3-port",
    "craftcms/redactor": "2.1.5",
    "sebastianlenz/linkfield": "1.0.11",
    "supercool/buttonbox": "2.0.2",
    "verbb/field-manager": "2.0.2",
    "barrelstrength/sprout-fields": "v3.1.7",
    "focuslabllc/craft-cheat-sheet": "2.0.3",
    "verbb/super-table": "2.0.8",
    "angellco/spoon": "3.2.0",
    "panlatent/craft-qiniu": "0.1.0",
    "lewisjenkins/craft-dynamic-fields": "3.0.6"
  },

  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/benjamminf/craft-neo.git"
    }
  ]
}
